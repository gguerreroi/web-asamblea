<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css" rel="stylesheet">
    <script src="https://kit.fontawesome.com/a81368914c.js"></script>
    <title>Resultados</title>
</head>
<body>
<div class="bg-white p-2 w-screen h-40 ">
    <div>
        <img src="/images/logo-micoope.png" alt="Logo MICOOPE" class="float-left w-32 p-2">
    </div>
    <div class="text-center">
        <h1 class="text-4xl text-blue-900 font-bold">XX Asamblea </h1>
        <p class="text-base font-semibold text-green-600 ">Cooperativa Coosanjer 2021</p>
    </div>
</div>
<p class="text-center text-3xl">Resultados</p>
<table class="min-w-full table-auto">
    <thead class="justify-between">
    <tr class="bg-blue-900">
        <th class="px-16 py-2"><span class="text-white">Id Votacion</span></th>
        <th class="px-16 py-2"><span class="text-white">Titulo Votacion</span></th>
        <th class="px-16 py-2"><span class="text-white">Candidato</span></th>
        <th class="px-16 py-2"><span class="text-white">Cantidad de votos</span></th>
    </tr>
    </thead>
    <tbody id="tbdata">

    </tbody>
</table>

<div id="results"></div>
<%- include('../partials/common.ejs'); -%>
<script
        src="https://code.jquery.com/jquery-3.6.0.js"
        integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
        crossorigin="anonymous"></script>
<script>
    $.ajax({
        url: `${URL_API}/asamblea/resultados`,
        method: 'get'
    }).then(response => {
        console.log(response)
        const {data} = response

        data.forEach(function(vot){
            let str = `<tr>
<td>${vot.CodVotacion}</td>
<td>${vot.StrTitulo}</td>
<td>${vot.StrDescripcion}</td>
<td>${vot.Conteo}</td>
</tr>`
            $("#tbdata").append(str)
        })
    })
</script>
</body>
</html>